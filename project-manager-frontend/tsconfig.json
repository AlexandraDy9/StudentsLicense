{
  "compilerOptions": {
    "jsx": "react",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "outDir": "./dist/",
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "lib": ["es6", "dom"],
    "module": "ES6",
    "allowJs": true
  },
  "include": [
    "./src/**/**/*"
  ]
}
